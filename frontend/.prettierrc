{
  "singleQuote": false,
  "tabWidth": 2,
  "jsxBracketSameLine": false,
  "arrowParens": "avoid",
  "semi": true,
  "printWidth": 100,
  "overrides": [
    {
      "files": "*.json",
      "options": {
        "parser": "json",
        "tabWidth": 2
      }
    },
    {
      "files": "*.md",
      "options": {
        "parser": "markdown",
        "tabWidth": 2
      }
    }
  ],
  "plugins": ["prettier-plugin-tailwindcss"]
}
